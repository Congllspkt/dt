TC용 SQL
SELECT U.MSG_DEFINITION_NM AS MSG_NM,
    B.ACTIVITY_SEQ AS COMPONENT_SEQ,
    B.ACTIVITY_NM,
    E.ACTIVITY_COMPONENT_NM,
    B.SEND_MDL_DEFINE_NM,
    B.NEXT_ACTIVITY_SEQ,
    B.NEXT_SEQ_UNDER_EXCEPTION,
    B.APPLICATION_MODULE_NM,
    B.APPLICATION_NAME,
    B.BATCH_PROCESS_ID,
    B.FILE_NM,
    B.FILE_DIR_PATH,
    DECODE(B.FILE_MODE,'NEW','신규생성','APPEND','APPEND','') AS FILE_MODE,
    B.MSG_DELIMITER,
    B.FILE_EXPLAIN,
    B.CONTEXT_NM,
    B.SOURCE_OBJ_NM,
    B.DML_TP,
    B.MSG_RETURN_TP,
    B.VO_NM,
    B.DML_WHERE_CLAUSE,
    B.DML_ORDER_BY_CLAUSE,
    B.COUNT_FLAG,
    B.BINDING_FLAG,
    B.FORWARD_FLAG,
    B.OLSTATR_NM,
    B.ACTIVITY_SEQ_FOR_TRUE,
    B.ACTIVITY_SEQ_FOR_FALSE,
    B.COMPANY,
    B.SENDER_NM,
    B.CALLBACK_NUM,
    B.RECEIVER_NM,
    B.RECEIVER_CP_NUM,
    B.SMS_DEFAULT_MSG,
    B.SMS_SEND_RESERVED_TIME,
    B.INTERFACE_ERR_CHK_RETURN_TP,
    B.COMMON_MODULE_NM,
    B.COMMON_MODULE_CLASS_NM,
    DECODE(B.DML_TRS_MODE_TP,'P','PostChange','C','Commit','') AS DML_TRS_MODE_TP ,
    U.EAI_RETRY_FLAG,
    DECODE(B.NUM_DATA_NULL_TP,'1','Zero->Null','2','Space->Zero','3','둘다적용','') AS NUM_DATA_NULL_TP,
    B.LOCK_FLAG,
    C.ACTIVITY_SEQ AS DETAILS_COMPONENT_SEQ,
    C.BINDING_SEQ AS DETAILS_BINDING_SEQ,
    DECODE(C.SOURCE_OBJ_TP,'M','Message','V','View','R','Row','H','HashMap','S','Sysdate','U','UserDefine') AS DETAILS_SOURCE_OBJ_TP,
    C.SOURCE_OBJ_NM AS DETAILS_SOURCE_OBJ_TP,
    C.SOURCE_OBJ_ATTR AS DETAILS_SOURCE_OBJ_ATTR,
    C.SOURCE_OBJ_ATTR_CONSTANT_V AS SOURCE_OBJ_ATTR_CONSTANT_V,
    DECODE(C.ATTR_DATA_TP,'1','String','2','Number','3','Date','NO') AS DETAILS_ATTR_DATA_TP,
    C.ATTR_NM AS DETAILS_ATTR_NM,
    D.ACTIVITY_SEQ AS REF_ATTRS_COMPONENT_SEQ,
    D.BINDING_SEQ AS REF_ATTRS_BINDING_SEQ,
    D.REFERENCE_ATTR_SEQ AS REFERENCE_ATTR_SEQ,
    D.ATTR_NM AS ATTR_NM,
    D.START_INDEX AS START_INDEX,
    D.END_INDEX AS END_INDEX,
    DECODE(D.SOURCE_OBJ_TP,'M','Message','U','UserDefine','C','CODECONVERT_ID','K','CODECONVERT_KEY','F','NEW->OLD(상수)','') AS REF_ATTRS_SOURCE_OBJ_TP,
    D.SOURCE_OBJ_ATTR AS REF_ATTRS_SOURCE_OBJ_ATTR,
    D.SOURCE_OBJ_ATTR_CONSTANT_V AS REF_SOURCE_OBJ_ATTR_CONSTANT_V,
    E.CLASS_NM 
    
    FROM TB_M00_USAGE U,
    TB_M00_INTERFACES A,
    TB_M00_MSG_ACTIVITIES B,
    TB_M00_ACTIVITY_COMPONENTS E,
    TB_M00_MSG_ACTIVITY_DETAILS C,
    TB_M00_MSG_ACTIVITY_REF_ATTRS  D
    WHERE 
    U.MSG_DEFINITION_NM LIKE 'H6C%' 
    AND U.GCS_COMP_CODE = '01'
    AND B.ACTIVITY_SEQ <> 0
    AND TO_CHAR(U.END_ACTIVE_DATE,'YYYYMMDD') = '29991231'
    AND U.MDL_DEFINE_ID = A.MDL_DEFINE_ID
    AND U.MSG_USAGE_ID = B.MSG_USAGE_ID(+) 
    AND B.COMPONENT_ID = E.COMPONENT_ID(+) 
    AND B.MSG_USAGE_ID = C.MSG_USAGE_ID(+) 
    AND B.ACTIVITY_SEQ = C.ACTIVITY_SEQ(+) 
    AND C.MSG_USAGE_ID = D.MSG_USAGE_ID(+) 
    AND C.ACTIVITY_SEQ = D.ACTIVITY_SEQ(+) 
    AND C.BINDING_SEQ = D.BINDING_SEQ(+)
    
    ORDER BY 
    u.MSG_DEFINITION_NM
    ,B.ACTIVITY_SEQ
    ,C.ACTIVITY_SEQ
    ,C.BINDING_SEQ
    ,D.ACTIVITY_SEQ
    ,D.BINDING_SEQ
    ,D.REFERENCE_ATTR_SEQ ;




SELECT
ACTIVITY_SEQ, ACTIVITY_NM , ACTIVITY_COMPONENT_NM , NEXT_ACTIVITY_SEQ, CLASS_NM
FROM TB_M00_MSG_ACTIVITIES A, TB_M00_USAGE B, TB_M00_ACTIVITY_COMPONENTS C
WHERE 1=1
  AND A.MSG_USAGE_ID  = B.MSG_USAGE_ID
  AND A.COMPONENT_ID = C.COMPONENT_ID
  AND B.MSG_DEFINITION_NM  = :1
  AND B.MSG_USAGE_ID = 
      (SELECT 
  MAX(MSG_USAGE_ID)
  FROM TB_M00_USAGE 
  WHERE MSG_DEFINITION_NM = :1
      )

;







